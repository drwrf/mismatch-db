{
    "name": "drwrf/mismatch-db",
    "description": "A simple library for complex SQL.",
    "license": "MIT",
    "authors": [
        {
            "name": "frwrd",
            "email": "hi@drwrf.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mismatch\\DB\\": [ "lib" ]
        },
        "files": [
            "lib/Expression.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mismatch\\DB\\": [ "test" ]
        }
    },
    "require": {
        "php": ">=5.4",
        "doctrine/dbal": "2.5.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*@dev",
        "phpunit/php-code-coverage": "2.0.*@dev",
        "phpunit/phpunit-mock-objects": "2.0.*@dev",
        "mockery/mockery": "0.9"
    },
    "archive": {
        "exclude": ["vendor", "test"]
    }
}
